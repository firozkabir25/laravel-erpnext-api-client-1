{
  "name": "hammock/laravel-erpnext-api-client",
  "description": "A simple client to integrate ERPNext with Laravel 5.",
  "keywords": [
    "laravel",
    "api",
    "erpnext"
  ],
  "homepage": "https://github.com/hammock/laravel-erpnext-api-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Ivanov",
      "email": "a.ivanov@gpsolutions.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "laravel/framework": "^6.",
    "php-curl-class/php-curl-class": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Hammock\\LaravelERPNext\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hammock\\LaravelERPNext\\ERPNextServiceProvider"
      ],
      "aliases": {
        "ERPNext": "Hammock\\LaravelERPNext\\Facades\\ERPNext"
      }
    }
  }
}
